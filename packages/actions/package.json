{
  "name": "@brebaje/actions",
  "version": "0.0.0",
  "description": "A set of actions and helpers for CLI, website and server commands",
  "author": "NicoSerranoP",
  "homepage": "https://github.com/NicoSerranoP/brebaje#readme",
  "license": "MIT",
  "keywords": [
    "typescript",
    "zero-knowledge",
    "zk-snarks",
    "phase-2",
    "trusted-setup",
    "ceremony",
    "snarkjs",
    "circom"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicoSerranoP/brebaje.git"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json"
  },
  "bugs": {
    "url": "https://github.com/NicoSerranoP/brebaje/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@adobe/node-fetch-retry": "^2.2.0",
    "@noble/hashes": "1.8.0",
    "cli-progress": "^3.12.0",
    "mime-types": "^3.0.1",
    "snarkjs": "^0.7.5"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/jest": "^29.5.14",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^25.0.3",
    "@types/node-fetch": "^2.6.13",
    "@types/snarkjs": "^0.7.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "d.ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testTimeout": 10000,
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1",
      "^src/(.*)": "<rootDir>/src/$1"
    }
  }
}
